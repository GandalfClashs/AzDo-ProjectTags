{
    "manifestVersion": 1,
    "id": "tags-for-projects-dev",
    "publisher": "MichaelKahle",
    "version": "1.0.77",
    "name": "Tags for Projects [DEV]",
    "description": "An extension to manage projects in Azure DevOps with tags. Supports filtering projects by tags. Tags can be visible for all users of an organization or user specific.",
    "icons": {
        "default": "static/tag_logo.png"
    },
    "categories": [
        "Azure Repos",
        "Azure Boards",
        "Azure Pipelines",
        "Azure Test Plans",
        "Azure Artifacts"
    ],
    "scopes": [
        "vso.project"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "files": [
        {
            "path": "static",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        },
        {
            "path": "images",
            "addressable": true
        },
        {
            "path": "node_modules/azure-devops-extension-sdk",
            "addressable": true,
            "packagePath": "lib"
        }
    ],
    "content": {
        "details": {
            "path": "README.md"
        }
    }
}
