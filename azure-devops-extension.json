{
    "manifestVersion": 1,
    "id": "tags-for-projects",
    "publisher": "MichaelKahle",
    "version": "1.0.83",
    "name": "Tags for Projects",
    "public": true,
    "description": "An extension to manage projects in Azure DevOps with tags. Supports filtering projects by tags. Tags can be visible for all users of an organization or user specific.",
    "repository": {
        "type": "git",
        "uri": "https://github.com/GandalfClashs/AzDo-ProjectTags"
    },
    "icons": {
        "default": "static/tag_logo.png"
    },
    "categories": [
        "Azure Repos",
        "Azure Boards",
        "Azure Pipelines",
        "Azure Test Plans",
        "Azure Artifacts"
    ],
    "tags": [
        "projects",
        "tags",
        "filter",
        "organize"
    ],
    "scopes": [
        "vso.project",
        "vso.profile"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "./LICENSE"
        }
    },
    "files": [
        {
            "path": "static",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        },
        {
            "path": "images",
            "addressable": true
        },
        {
            "path": "node_modules/azure-devops-extension-sdk",
            "addressable": true,
            "packagePath": "lib"
        }
    ]
}